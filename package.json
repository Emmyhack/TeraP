{
  "name": "terap-universal",
  "version": "1.0.0",
  "description": "TeraP - Universal Therapist and Wellness DAO Platform",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "compile": "forge build",
    "test": "forge test",
    "deploy": "forge script script/ZetaChainProxyDeploy.s.sol --rpc-url https://zetachain-athens-evm.blockpi.network/v1/rpc/public --broadcast",
    "deploy:testnet": "forge script script/ZetaChainProxyDeploy.s.sol --rpc-url https://zetachain-athens-evm.blockpi.network/v1/rpc/public --broadcast --verify",
    "deploy:mainnet": "forge script script/ZetaChainProxyDeploy.s.sol --rpc-url https://zetachain-evm.blockpi.network/v1/rpc/public --broadcast --verify",
    "localnet:start": "zetachain localnet start --chains solana bitcoin sui ton",
    "localnet:stop": "zetachain localnet stop",
    "localnet:check": "zetachain localnet check",
    "accounts:list": "zetachain accounts list --json",
    "accounts:create": "zetachain accounts create --type evm --name terap-deployer",
    "query:balances": "zetachain query balances --name terap-deployer --network testnet",
    "query:chains": "zetachain query chains list --json",
    "query:fees": "zetachain query fees --json",
    "faucet": "zetachain faucet --name terap-deployer"
  },
  "keywords": [
    "web3",
    "dao",
    "wellness",
    "therapy",
    "zetachain",
    "omnichain"
  ],
  "author": "TeraP Team",
  "license": "MIT",
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@mysten/sui.js": "^0.50.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@openzeppelin/contracts-upgradeable": "^5.0.0",
    "@rainbow-me/rainbowkit": "^2.0.0",
    "@solana/wallet-adapter-react": "^0.15.0",
    "@solana/wallet-adapter-wallets": "^0.19.0",
    "@solana/web3.js": "^1.87.0",
    "@ton-api/client": "^0.4.0",
    "@ton/core": "^0.62.0",
    "@tonconnect/sdk": "^3.3.1",
    "@tonconnect/ui-react": "^2.0.0",
    "@zetachain/protocol-contracts": "^11.0.0",
    "@zetachain/toolkit": "^16.2.2",
    "crypto-js": "^4.2.0",
    "ethers": "^6.15.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.400.0",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3.4.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "uuid": "^13.0.0",
    "viem": "^2.0.0",
    "wagmi": "^2.0.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.0",
    "typescript": "^5.0.0"
  }
}
